@using Querystringproject.Models;


@{

    <table class="table">
        <tr><th>Name</th><th>RollNo</th><th>Mobile</th><th>Edit</th><th>Insert</th><th>Delete</th></tr>
        @{
            foreach (Record item in (List<Record>)Model)
            {
                <tr>
                    <td>@item.Name</td>
                    <td>@item.Roll</td>
                    <td>@item.Mobile </td>
                    <td><a href="/Record/EditingDetails?roll=@item.Roll">Editing</a></td>
                    <td><a href="/Record/AddData?roll=@item.Roll">Insertion</a></td>
                    <td><a href="/Record/DeleteDetails?roll=@item.Roll">Deleting</a></td>



                </tr>
            }  
        }
    </table>         

}
